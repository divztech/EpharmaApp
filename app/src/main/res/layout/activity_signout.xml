<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".signout">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg4"/>

    <Button
        android:id="@+id/signoutbutton"
        android:layout_width="175dp"
        android:layout_height="42dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="488dp"
        android:background="#B4181419"
        android:fontFamily="@font/domine"
        android:text="SIGN OUT"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pay_label3"
        android:layout_width="281dp"
        android:layout_height="33dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="120dp"
        android:fontFamily="@font/domine"
        android:text="Payment Confirmed !"
        android:textColor="@color/teal_700"
        android:textSize="27sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pay_label2"
        android:layout_width="188dp"
        android:layout_height="41dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="200dp"
        android:fontFamily="@font/domine"
        android:text="Order Placed. "
        android:textColor="@color/teal_700"
        android:textSize="27sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pay_label4"
        android:layout_width="188dp"
        android:layout_height="41dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="356dp"
        android:fontFamily="@font/domine"
        android:text="THANK YOU !"
        android:textColor="@color/teal_700"
        android:textSize="27sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="88dp"
        android:layout_height="82dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="88dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logout" />

</androidx.constraintlayout.widget.ConstraintLayout>